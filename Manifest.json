{
    "name": "Chrome Bookmarks",
    "version": "0.1.0",
    "description": "rahhh",
    "permission": ["storage", "tabs"],
    "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/bookmark.png",
        "assets/play.png",
        "assets/delete.png",
        "assets/save.png"
      ]
    }
  ],
  "action": {
    "default_icon": {
      "16": "assets/ext-icon.png",
      "24": "assets/ext-icon.png",
      "32": "assets/ext-icon.png"
    },
    "default_title": "Chrome Bookmarks",
    "default_popup": "UI.html"
  },
  "manifest_version": 3
}